		char a[]=new char[len];
		int vowels=0; 
		for(int i=0;i<len;i++)
			{ 
				a[i]=name.charAt(i); 
				if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u' 
					||a[i]=='A'||a[i]=='E'||a[i]=='I'||a[i]=='O'||a[i]=='U') 
					vowels=vowels+1;
			} 
		System.out.println(vowels);
	}
}
